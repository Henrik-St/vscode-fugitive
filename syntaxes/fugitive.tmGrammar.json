{
    "scopeName": "source.fugitive",
    "patterns": [
        {
            "name": "keyword.control.titles",
            "match": "Head|Merge|Help|Unstaged|Staged|Unpushed"
        },
        {
            "name": "variable.parameter",
            "match": "(?<=Head:) .*\n|(?<=Merge:) .*\n|(?<=Unpushed to) .*(?=\\(\\w\\):\n)"
        },
        {
            "name": "constant.language",
            "match": "(?<=Help:) .*\n"
        },
        {
            "name": "constant.numeric",
            "match": "(?<=Staged \\().(?=\\):\n)"
        },
        {
            "name": "constant.numeric",
            "match": "(?<=\\().(?=\\):\n)"
        },
        {
            "name": "constant.character.escape",
            "match": "M |D |R "
        }
    ]
}